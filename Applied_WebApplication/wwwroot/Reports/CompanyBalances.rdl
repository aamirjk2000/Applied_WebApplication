  <DataSets>
    <DataSet Name="ds_ComBalance">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
[L].[Customer],
[C].[Code],
[C].[Title],
[N].[Title] [Nature],
CAST(ROUND(SUM([L].[DR]), 2) AS FLOAT) AS [DR],
CAST(ROUND(SUM([L].[CR]), 2) AS FLOAT) AS [CR],
CAST(ROUND(SUM([L].[DR] -[L].[CR]), 2) AS FLOAT) AS [BAL]
FROM[Ledger] [L] LEFT JOIN [Customers] [C] on[C].[ID] = [L].[Customer]
LEFT JOIN 
(
  SELECT [Key] AS[ID],[Value] AS [Title]
  FROM[Directories] 
  WHERE Directory = 'CompanyStatus'
)
  [N] ON [N].[ID] = [C].[Status]
  WHERE [COA] IN(3,6,52,53,17,18)    
  GROUP BY[Customer]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

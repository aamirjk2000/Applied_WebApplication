  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Source_Msool</DataSourceName>
        <CommandText>SELECT 
[L].[COA],
[A].[Title] AS [Accounts],
[L].[Customer] AS [CustomerID],
[C].[Title] AS [Customer],
CAST(SUM([L].[DR]) AS FLOAT) AS [DR],
CAST(SUM([L].[CR]) AS FLOAT) AS [CR]
FROM [Ledger] [L]
LEFT JOIN [COA] [A]      ON [A].[ID] = [L].[COA]
LEFT JOIN [Customers] [C] ON [C].[ID] = [L].[Customer]
GROUP BY [COA],[Customer]</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="COA">
          <DataField>COA</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Accounts">
          <DataField>Accounts</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CustomerID">
          <DataField>CustomerID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Customer">
          <DataField>Customer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DR">
          <DataField>DR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CR">
          <DataField>CR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Amount">
          <Value>=Fields!DR.Value - Fields!CR.Value</Value>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
